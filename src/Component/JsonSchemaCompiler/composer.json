{
  "name": "jane-php/json-schema-compiler",
  "description": "Jane's JSON Schema compiler component",
  "license": "MIT",
  "authors": [
    {
      "name": "Baptiste Leduc",
      "email": "baptiste.leduc@gmail.com"
    }
  ],
  "require": {
    "php": "^8.2",
    "doctrine/inflector": "^2.0",
    "jane-php/json-schema-metadata": "^8.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0"
  },
  "autoload": {
    "psr-4": {
      "Jane\\Component\\JsonSchemaCompiler\\": ""
    },
    "exclude-from-classmap": [
      "/Tests/"
    ]
  },
  "config": {
    "process-timeout": 1800,
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
