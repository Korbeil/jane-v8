{
  "name": "jane-php/json-schema-metadata",
  "description": "Jane's JSON Schema metadata component",
  "license": "MIT",
  "authors": [
    {
      "name": "Baptiste Leduc",
      "email": "baptiste.leduc@gmail.com"
    }
  ],
  "require": {
    "php": "^8.1",
    "jane-php/json-schema-parser": "^8.0",
    "league/uri": "^6.8",
    "php-jsonpointer/php-jsonpointer": "^4.0",
    "symfony/filesystem": "^6.2"
  },
  "autoload": {
    "psr-4": {
      "Jane\\Component\\JsonSchemaMetadata\\": ""
    },
    "exclude-from-classmap": [
      "/Tests/"
    ]
  },
  "config": {
    "process-timeout": 1800,
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
