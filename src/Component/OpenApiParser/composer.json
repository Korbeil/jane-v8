{
  "name": "jane-php/open-api-parser",
  "description": "Jane's OpenAPI parser component",
  "license": "MIT",
  "authors": [
    {
      "name": "Baptiste Leduc",
      "email": "baptiste.leduc@gmail.com"
    }
  ],
  "require": {
    "php": "^8.2",
    "symfony/filesystem": "^7.0",
    "symfony/serializer": "^7.0",
    "symfony/yaml": "^7.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0"
  },
  "autoload": {
    "psr-4": {
      "Jane\\Component\\OpenApiParser\\": ""
    },
    "exclude-from-classmap": [
      "/Tests/"
    ]
  },
  "scripts": {
    "tests": "phpunit"
  },
  "config": {
    "process-timeout": 1800,
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
