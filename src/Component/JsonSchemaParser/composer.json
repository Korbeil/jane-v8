{
  "name": "jane-php/json-schema-parser",
  "description": "Jane's JSON Schema parser component",
  "license": "MIT",
  "authors": [
    {
      "name": "Baptiste Leduc",
      "email": "baptiste.leduc@gmail.com"
    }
  ],
  "require": {
    "php": "^8.1",
    "symfony/filesystem": "^6.2",
    "symfony/serializer": "^6.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0"
  },
  "autoload": {
    "psr-4": {
      "Jane\\Component\\JsonSchemaParser\\": ""
    },
    "exclude-from-classmap": [
      "/Tests/"
    ]
  },
  "scripts": {
    "tests": "phpunit"
  },
  "config": {
    "process-timeout": 1800,
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
